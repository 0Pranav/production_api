ems) %}\\n        <table class=\\\"table table-sm table-bordered\\\">\\n            {% for i in item_details %}\\n            <tr>\\n                {% if i.variants[0].primary_attribute %}\\n                <td>\\n                    <div class=\\\"row\\\">\\n                        <div class=\\\"col-xs-3\\\">\\n                            Item: \\n                            <strong>{{ i.item }}</strong>\\n                        </div>\\n                        <div class=\\\"col-xs-4\\\">\\n                            Delivery Location: \\n                            <strong>{{ i.delivery_location }}</strong>\\n                        </div>\\n                        <div class=\\\"col-xs-5\\\">\\n                             Delivery Date:\\n                            <strong>{{ i.delivery_date }}</strong>\\n                        </div>\\n                    </div>\\n                    <table class=\\\"table table-sm table-bordered\\\">\\n                        <tr>\\n                            <th>S.No.</th>\\n                            {% for attr in i.variants[0].attributes %}\\n                            <th>{{ attr }}</th>\\n                            {% endfor %}\\n                            {% for attr in i.variants[0]['values'] %}\\n                                {% if attr == 'default' %}\\n                                <th>Quantity</th>\\n                                {% else %}\\n                                <th>{{ attr }}</th>\\n                                {% endif %}\\n                            {% endfor %}\\n                        </tr>\\n                        {% for variant in i.variants %}\\n                        <tr>\\n                            <td>{{ loop.index }}</td>\\n                            {% for attr in variant.attributes %}\\n                            <td>{{ variant.attributes[attr] }}</td>\\n                            {% endfor %}\\n                            {% for attr in variant['values'] %}\\n                            <td>\\n                                {% if variant['values'][attr].qty %}\\n                                <div>\\n                                    {{ variant['values'][attr].qty }}{{ ' ' + i.default_uom}}\\n                                    {% if variant['values'][attr].secondary_qty %}\\n                                    <span>\\n                                        <br>\\n                                        {{ variant['values'][attr].secondary_qty }}{{ ' ' + i.secondary_uom}}\\n                                    </span>\\n                                    {% endif %}\\n                                    <br>\\n                                    Rate: {{ frappe.utils.fmt_money(variant['values'][attr].rate, currency=\\\"\\u20b9\\\") }}\\n                                </div>\\n                                {% else %}\\n                                <div>---</div>\\n                                {% endif %}\\n                            </td>\\n                            {% endfor %}\\n                        </tr>\\n                        {% endfor %}\\n                        \\n                    </table>\\n                </td>\\n                {% else %}\\n                <td>\\n                    <table class=\\\"table table-sm table-bordered\\\">\\n                        <tr>\\n                            <th>S.No.</th>\\n                            <th>Item</th>\\n                            {% for attr in i.variants[0].attributes %}\\n                            <th>{{ attr }}</th>\\n                            {% endfor %}\\n                            <th>Quantity</th>\\n                            <th>Rate</th>\\n                            <th>Delivery Location</th>\\n                            <th>Delivery Date</th>\\n                        </tr>\\n                        {% for variant in i.variants %}\\n                        <tr>\\n                            <td>{{ loop.index }}</td>\\n                            <td>{{ i.item }}</td>\\n                            {% for attr in variant.attributes %}\\n                                <td>{{ variant.attributes[attr] }}</td>\\n                            {% endfor %}\\n                            <td>\\n                                {{ variant['values']['default'].qty }}{{ ' ' + i.default_uom}}\\n                                {% if variant['values']['default'].secondary_qty %}\\n                                <span>\\n                                    <br>\\n                                    ({{ variant['values']['default'].secondary_qty }}{{ ' ' + i.secondary_uom }})\\n                                </span>\\n                                {% endif %}\\n                            </td>\\n                            <td>{{ frappe.utils.fmt_money(variant['values']['default'].rate, currency=\\\"\\u20b9\\\") }}</td>\\n                            <td>{{ i.delivery_location }}</td>\\n                            <td>{{ i.delivery_date }}</td>\\n                        </tr>\\n                        {% endfor %}\\n                    </table>\\n                </td>\\n                {% endif %}\\n            </tr>\\n            {% endfor %}\\n        </table>\\n    </div>\\n    <div class=\\\"row\\\" style=\\\"border-bottom:1px solid #666;\\\">\\n        {% if doc.comments %}\\n        <div class=\\\"col-xs-12\\\">\\n            PO Comments: {{doc.comments}}\\n        </div>\\n        {% endif %}\\n    </div>\\n    <div class=\\\"row\\\">\\n        {% set company = frappe.db.get_single_value(\\\"Company Settings\\\", \\\"company\\\") %}\\n        <div class=\\\"col-xs-6 text-center\\\" style=\\\"border-right:0px solid #666;\\\">\\n        </div>\\n        <div class=\\\"col-xs-6 text-right\\\">\\n            <small>For </small>\\n            <strong>{{ company  }}</strong>\\n            <br>\\n            <br>\\n            <br> Authorised Signatory\\n        </div>\\n    </div>\\n</div>\